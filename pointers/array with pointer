#include <stdio.h>
int *findmid(int a[],int n){
    return &a[n/2];
}
int main()
{
    int a[]={1,2,3,4,5};
    int sum=0,*p;
    for(p=&a[0]; p<=&a[4];p++){
        sum+=*p;
    }
    printf("%d",sum);
}

#include <stdio.h>
int *findmid(int a[],int n){
    return &a[n/2];
}
int main()
{
    int a[]={1,2,3,4,5};
    int sum=0,*p;
    for(p=a; p<=a+4;p++){
        sum+=*p;
    }
    printf("%d",sum);
}



